{
	"extends": "astro/tsconfigs/strict",
	"include": [
		".astro/types.d.ts",
		"**/*",
		".commitlintrc.ts",
		".prettierrc.mjs",
		"eslint.config.mjs"
	],
	"exclude": ["dist"],
	"compilerOptions": {
		"baseUrl": ".",
		"jsx": "react-jsx",
		"jsxImportSource": "react",
		"paths": {
			"@/*": ["./src/*"]
		}
	}
}
